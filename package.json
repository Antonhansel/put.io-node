{
  "name": "put.io-node",
  "version": "0.0.1",
  "description": "A Node.js wrapper for put.io APIv2 http://put.io",
  "main": "js/index.js",
  "scripts": {
    "postinstall": "typings install",
    "preversion": "npm run compile && npm test",
    "precompile": "npm run clean",
    "compile": "tsc",
    "test": "env NODE_ENV=test ./node_modules/.bin/mocha --recursive js/tests",
    "clean": "del-cli js/",
    "lint": "tslint 'ts/**/*.ts?(x)' || exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alormil/put.io-node.git"
  },
  "keywords": [
    "put.io",
    "npm",
    "module",
    "package",
    "node"
  ],
  "author": "Alain Lormil <alormil@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alormil/put.io-node/issues"
  },
  "homepage": "https://github.com/alormil/put.io-node#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "del-cli": "^0.2.0",
    "mocha": "^3.0.2",
    "tslint": "^3.15.1",
    "tslint-microsoft-contrib": "^2.0.10",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}
